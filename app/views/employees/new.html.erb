<h1>New Employee</h1>

<%= form_with model: @employee do |form| %>
    
    <%= form.label :employer_name, "Employer Name"%>
    <%= form.text_field :employer_name %>

    <%= form.label :employee_position, "Position"%>
    <%= form.text_field :employee_position %>
    
    <%= form.submit %>
<% end %>
<td><%= link_to "Back", employee_path(User.get_current_user(current_account).user_id) , class:"btn btn-primary"%></td>