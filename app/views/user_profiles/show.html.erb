<%# <%= link_to("<< Back to List", subjects_path, :class => 'back-link') %> %>

<div class="subjects show">
  <h2>Show Profile</h2>

  <table summary="Subject detail view">
    <tr>
      <th>First name</th>
      <td><%= @user_profile.user_first_name  %></td>
    </tr>
    <tr>
      <th>Last name</th>
      <td><%= @user_profile.user_last_name %></td>
    </tr>
    <tr>
      <th>Email</th>
      <td><%= @email %></td>
    </tr>
    <tr>
      <th>Membership</th>
      <td><%= @membership %></td>
    </tr>
    <tr>
      <th>FaceBook</th>
      <td><%= @user_profile.user_facebook_profile_url %></td>
    </tr>
    <tr>
      <th>Instagram</th>
      <td><%= @user_profile.user_instagram_profile_url %></td>
    </tr>
    <tr>
      <th>Linkedin</th>
      <td><%= @user_profile.user_linkedin_profile_url %></td>
    </tr>
    <tr>
      <th>Graduating Year</th>
      <td><%= @user_profile.user_graduating_year %></td>
    </tr>
    <tr>
      <th>About me</th>
      <td><%= @user_profile.user_about_me_description %></td>
    </tr>
    <tr>
      <th>Phone Number</th>
      <td><%= @user_profile.user_phone_number %></td>
    </tr>
    <tr>
      <th>Selfie</th>
      <td><%= image_tag image_url(@user_profile.user_profile_picture) %></td>
    </tr>

  </table>

</div>